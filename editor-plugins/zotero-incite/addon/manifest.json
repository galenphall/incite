{
	"manifest_version": 2,
	"name": "inCite for Zotero",
	"version": "0.2.0",
	"description": "Library sync control center for inCite, powered by your Zotero library.",
	"author": "Galen Hall",
	"homepage_url": "https://github.com/galenphall/incite",
	"icons": {
		"48": "content/icons/icon48.png",
		"96": "content/icons/icon96.png"
	},
	"preferences": "content/preferences.xhtml",
